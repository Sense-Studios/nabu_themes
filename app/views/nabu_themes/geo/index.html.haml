:javascript
  // these need to be set in a rb parsed file
  window.main_color = '#{@theme.main_color}';
  window.support_color = '#{@theme.support_color}';
  window.background_color = '#{@theme.background_color}';
  window.menudata = #{@menudata};
  window.menuconfig = #{@menuconfig};
  window.channelsettings = #{@channelsettings};

/ necc. files, let's keep this small
= javascript_include_tag "//code.jquery.com/jquery-2.2.0.min.js"
= javascript_include_tag "nabu_themes/geo/geo"
= stylesheet_link_tag "nabu_themes/geo/geo", media: "all", "data-turbolinks-track" => true, "async" => true

/ le map (and as you notice, the only actual html here)
#map

/ Google script
%script{:src=>"https://maps.googleapis.com/maps/api/js?key=#{GEO_GOOGLE_API_KEY}&callback=initMap", :async=>true, :defer=>true}

